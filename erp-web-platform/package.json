    "lint": "vue-cli-service lint",
    "eslint-fix": "eslint src/**/**/*.vue --fix",
    "format-code": "prettier-eslint --write \"src/**/*.js\" \"src/**/*.vue\"",
    "release": "release-it",
    "start": "npm run serve",
    "analyzer": "cross-env use_analyzer=true npm run serve",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "doc-gen": "yarn vue-docgen src/components/**/*.vue doctmp",
    "push": "node cli/submit",
    "trans": "prettier --write \"src/lang/module/**\" && node cli/translation.js"
