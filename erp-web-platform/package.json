  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --report && node cli/write",
    "build:test": "vue-cli-service build --mode testing && node cli/write",
    "build:test-2": "vue-cli-service build --mode testing2 && node cli/write",
    "build:dev": "vue-cli-service build --mode dev && node cli/write",
    "build:uat": "vue-cli-service build --mode uat && node cli/write",
